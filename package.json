{
  "name": "projeto-fiap",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate:e2e": "prisma generate --schema=prisma-e2e/schema.prisma",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev:migrate": "npm run prisma:generate && ts-node-dev src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "format": "prettier --ignore-path .prettierignore --write .",
    "test:e2e": "npm run prisma:generate:e2e && jest --runInBand --config=jest.e2e.config.ts",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
